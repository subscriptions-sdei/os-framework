.dropdown-menu li>a { display:block; padding:8px 20px; clear:both; font-weight:normal; line-height:100%; color:#333333; white-space:nowrap; font-size:14px; }
.dropdown-menu li>a:hover, .dropdown-menu li>a:focus, .dropdown-submenu:hover>a { text-decoration:none; color:white; 
background: #eb650b; /* Old browsers */
background: -moz-linear-gradient(top, #eb650b 0%, #d3570a 100%); /* FF3.6+ */
