filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#eb650b', endColorstr='#d3570a',GradientType=0 ); /* IE6-9 */
}

.dropdown-menu .active>a, .dropdown-menu .active>a:hover { color:#333333; text-decoration:none; outline:0; 
background: #eb650b; /* Old browsers */
background: -moz-linear-gradient(top, #eb650b 0%, #d3570a 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#eb650b), color-stop(100%,#d3570a)); /* Chrome,Safari4+ */
