.showroom-cat {border-bottom: 1px solid #E6E6E6; margin-bottom: 15px;}
.showroom-cat h2{font-size: 18px; text-transform: uppercase; margin-bottom: 10px; font-weight: bold;}
.showroom-cat h2 a{float:right; text-transform: none; font-weight:normal;}

